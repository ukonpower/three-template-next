{
	"extends": [
		"next/core-web-vitals",
		"plugin:import/recommended",
		"plugin:import/warnings"
	],
	"rules": {
		"import/order": [
			"error",
			{
				"alphabetize": {
					"order": "asc"
				}
			}
		],
		"import/named": "off"
	},
	"overrides": [
		{
			"files": [
				"*.ts"
			],
			"extends": [
				"mdcs"
			],
			"rules": {
				"no-unused-vars": "off"
			}
		}
	]
}